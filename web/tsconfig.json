{
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist", "web"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "@c/*": ["src/components/*"],
      "@js/*": ["src/js/*"],
      "@svg/*": ["src/components/svg/*"],
      "@package/*": ["package/*"],
      "@slider/*": ["src/js/slider/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "declaration": true
  }
}
