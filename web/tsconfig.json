{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "@c/*": ["src/components/*"],
      "@js/*": ["src/js/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
